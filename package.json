{
  "name": "react-starter",
  "version": "1.0.2",
  "description": "typescript",
  "main": "index.js",
  "scripts": {
    "common": "cross-env NODE_ENV=development webpack --config ./scripts/config/webpack.common.js",
    "server": "cross-env NODE_ENV=development node scripts/server",
    "json-server": "npx json-server server/db.json",
    "dev": "cross-env NODE_ENV=development webpack serve --config ./scripts/config/webpack.dev.js",
    "build": "cross-env NODE_ENV=production webpack --config ./scripts/config/webpack.prod.js",
    "lint-staged": "npm run lint-eslint && npm run lint-stylelint",
    "lint-eslint": "eslint -c .eslintrc.js --ext .ts,.tsx,.js src",
    "lint-stylelint": "stylelint --config .stylelintrc.js src/**/*.{less,css,scss}",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --config .eslintrc.js"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.js"
    ],
    "*.{ts,tsx,js,jsx,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "ie >= 11",
    "not op_mini all"
  ],
  "repository": {
    "type": "git",
    "url": "git+http://github.com/yyl134934/ts.git"
  },
  "keywords": [],
  "author": "junger",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yyl134934/ts/issues"
  },
  "homepage": "https://github.com/yyl134934/ts#readme",
